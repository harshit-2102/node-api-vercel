<%- include('partials/header'); -%>

    <div class="pb-3 mb-3 d-flex border-bottom container">
        <h4 class="m-0 p-0 me-1">All Photos</h4>
        <h5 class="m-0 p-0"> (<%= count %>)</h5>
    </div>
    <div class="container admin-user-table">

        <table class="table table-striped table-hover table-responsive">
            <thead>
                <tr>
                    <th scope="col">Photo</th>
                    <th scope="col">Hashtags</th>
                    <th scope="col">Exposure Value</th>
                    <th scope="col">Photo Value</th>
                </tr>
            </thead>
            <tbody class="">
                <% for (let data of datas) { %>

                    <tr class="position-relative">
                        <td scope="row">
                            <img src="<%= data.imageUrl %>" alt="" style="width: 100px;">
                        </td>
                        <td style="max-width: 120px;">
                            <% for (let hashtag of data.hashtags) { %>
                                <span><%= hashtag %></span> 
                                <% } %>
                        </td>
                        <td>
                            <%= data.ev %>
                        </td>
                        <td>
                            <%= data.sp %>
                        </td>
                    </tr>

                    <% } %>
            </tbody>
        </table>
    </div>

    <%- include('partials/footer'); -%>